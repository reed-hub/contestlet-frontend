# üèÜ Production Environment Configuration
# Connects to PRODUCTION backend for live operations

# API Configuration
REACT_APP_API_BASE_URL=https://api.contestlet.com
REACT_APP_ENVIRONMENT=production

# Debug Settings (disabled for production)
REACT_APP_DEBUG_MODE=false
GENERATE_SOURCEMAP=false

# Production Features (minimal debugging)
REACT_APP_SHOW_DEBUG_INFO=false
REACT_APP_ENABLE_CONSOLE_LOGS=false

# Real Services (full production)
REACT_APP_MOCK_SMS=false

# Production Monitoring
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
